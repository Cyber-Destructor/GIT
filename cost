print("Soham S Shinde,63")
print("Using R execute the basic commands,array,list and frames.")
#basic commands
print("Hello World")
#adding two numbers
print(60+50)
#Arrays
#create two vector of different lengths
print("create two vector of different lengths.")
v1<-c(5,9,3)
print(v1)
v2<-c(10,11,12,13,14,15)
print(v2)
#taking these two vectors as inputs to the array
print("taking these two vectors as inputs to the array")
result<-array(c(v1,v2),dim=c(3,3,2))
print(result)
#list
#creating a list containing string,numbers,vectors and logical values
print("creating a list containing string,numbers,vectors and logical values")
list1<-list("Blue","Yellow",c(1,2,3),44.3,321.3,TRUE)
print(list1)
#FRAMES
#create a frame containing student data
print("create a frame containing student data")
frame1<-c("Raj","Sona","Ram","Siya")
marks<-runif(4,50,100)
report<-data.frame(Name=frame1,Marks=marks)
print(report)

4
print("Soham S Shinde,63")
print("Using R execute the statistical functions :mean,median,mode,quartile range,inter quartile histrogram.")
v=c(4,7,8,9,6,2,1,7,4,7)
paste("Mean:",mean(v))
paste("Median:",median(v))
paste("Mode:",mode(v))
paste("Range:",(max(v)-min(v)))
paste("Quartile 1:",quantile(v,0.25))
paste("Quartile 2:",quantile(v,0.5))
paste("Quartile 3:",quantile(v,0.75))
paste("Inter-Quartile Range:",IQR(v))

