print("Soham S Shinde,63")
print ("Create a mataix using R and perform the operations of addition, transpose and multiplicd operations")
m1<-matrix (c(1:9),nrow = 3, byrow = TRUE)
print (m1)
m2<-matrix (c(4:12), nrow = 3,byrow = TRUE)
print (m2)
print ("addition of m1 and m2")
print (m1+m2)
print("transpose of m1")
print (t(m1))
print ("transpose of m2")
print (t(m2))
print("multiplication of m1 and m2")
print (m1* m2)

