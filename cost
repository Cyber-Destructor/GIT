print("Soham S Shinde, 63")
x<-c(2.5,3,9,8,11,6)
count <-0
for (val in x)
{
  if (val%%2==0) count=count +1
}
print (count)
i<-1
while (i<6)
{
  print(i)
  i=i+1
}
x<-1.5
for(val in x)
{
  if (val==3)
  {
    break
  }
  print(val)
}
x<-1.5
for (val in x)
  if (val ==3){
    next
  }
print (val)
x<-1
repeat {
  print (x)
  x=x+1
  if(x ==6){
    break
  }
}
