print("Soham S Shinde, 63")
print("Create a matrix using R and perform operations like addition,inverse,transpose and multiplication")
matA=matrix(c(10:18),nrow=3,byrow=TRUE)
matB=matrix(c(1:9),nrow=3,byrow=TRUE)
print("Matrix A is:")
print(matA)
print("Matrix B is:")
print(matB)
print("Addition of 2 matrix are:")
print(matA+matB)
print("Substraction of 2 matrix are:")
print(matA-matB)
print("Multiplication of 2 matrix are:")
print(matA*matB)
print("Division of 2 matrix are:")
print(matA/matB)
print("Modulus of 2 matrix are:")
print(matA%%matB)
print("Transpose of matrix A")
print(+(matA))
print("Transpose of matrix B")
print(+(matB))

